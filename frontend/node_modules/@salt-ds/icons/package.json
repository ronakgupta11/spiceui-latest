{
  "name": "@salt-ds/icons",
  "version": "1.13.2",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpmorganchase/salt-ds.git",
    "directory": "packages/icons"
  },
  "bugs": "https://github.com/jpmorganchase/salt-ds/issues",
  "main": "dist-cjs/index.js",
  "files": [
    "css",
    "saltIcons.css",
    "dist-cjs",
    "dist-es",
    "dist-types",
    "CHANGELOG.md"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
